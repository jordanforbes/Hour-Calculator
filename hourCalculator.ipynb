    "def hoursLeft(shift):\n",
    "    return 24 - shift\n",
    "\n",
    "def sleepTime(shift,sleep):\n",
    "    timeLeft = hoursLeft(shift)-sleep\n",
    "    return timeLeft\n",
    "\n",
    "print(hoursLeft(8),\"in the day\")\n",
    "print(sleepTime(12,8),\"not including sleep\")"
   ]
